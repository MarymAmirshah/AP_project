    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <children>
      <Button fx:id="bu" alignment="TOP_CENTER" mnemonicParsing="false" onMouseClicked="#controlPanel" text="controlPanel" />
      <Button mnemonicParsing="false" onMouseClicked="#productList" text="list of products" />
   </children>
</VBox>
